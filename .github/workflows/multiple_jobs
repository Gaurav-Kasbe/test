name: My First Workflow

on: [push]

jobs:
 Build:
   runs-on: ubuntu-latest

   steps:
     - name: Code Checkout Repo
       uses: actions/checkout@v5

     - name: Building
       run: echo "this is test cases"

     - name: Genrating report
       run: echo "ALLTEST CASES PASSED....!" > output.txt

     - name: upload artifact
       uses: actions/upload-artifact@v4
       with:
         name: test-report
         path: output.txt
   